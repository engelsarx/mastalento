<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="theme-color" content="#393E46">
    <title>@ViewBag.Title - @System.Configuration.ConfigurationManager.AppSettings["Brand"]</title>
    <link href="https://fonts.googleapis.com/css?family=Raleway:300,400,700" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.linearicons.com/free/1.0.0/icon-font.min.css">
    <script type="text/javascript" src="https://kit.fontawesome.com/2426ff205a.js" crossorigin="anonymous"></script>
    <script type="text/javascript" src="https://cdn.tiny.cloud/1/824zdtala42g2d6ezizwi8aytcnwccdqljhodg08p8esv46q/tinymce/5/tinymce.min.js" referrerpolicy="origin"></script>
    <script type="text/javascript" src="~/Scripts/app/tinymce/init.js"></script>
    @Styles.Render("~/Content/Site.css")
    @Styles.Render("~/Content/css/data-tables")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    @Html.Partial("_Header")

    <main class="" id="content" role="main">
        @RenderBody()
    </main>

    @Scripts.Render("~/bundles/jquery")
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/data-tables")
    @Scripts.Render("~/bundles/datetimepickers")
    @RenderSection("scripts", required: false)
</body>
</html>
